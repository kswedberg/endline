{
  "name": "endline",
  "displayName": "endline",
  "description": "insert punctuation at end of line",
  "version": "0.0.1",
  "engines":
  {
    "vscode": "^1.31.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onCommand:extension.helloWorld"],
  "main": "./extension.js",
  "contributes":
  {
    "commands": [
      {
        "command": "extension.endline",
        "title": "End line with punctuation"
      },
      {
        "command": "extension.newline",
        "title": "End line with punctuation and go to new line"
      }
    ]
  },
  "scripts":
  {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies":
  {
    "eslint": "^5.13.0",
    "eslint-config-kswedberg": "^3.0.0",
    "vscode": "^1.1.28"
  },
  "license": "MIT"
}
